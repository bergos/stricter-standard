  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bergos/stricter-standard/issues"
  },
  "homepage": "https://github.com/bergos/stricter-standard",
  "dependencies": {
    "eslint": "^9.23.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "standard-engine": "^15.1.0"
  },
  "devDependencies": {
    "c8": "^10.1.2",
    "mocha": "^11.0.1",
    "stricter-standard": "^0.3.0"
